{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "barryvdh/laravel-ide-helper": "^2.15.1",
        "filament/filament": "^3.2.46",
        "filament/spatie-laravel-media-library-plugin": "^3.2.46",
        "filament/spatie-laravel-settings-plugin": "^3.2.46",
        "filament/widgets": "^3.2.46",
        "guzzlehttp/guzzle": "^7.8.1",
        "joshembling/image-optimizer": "^1.3.1",
        "laravel/framework": "^10.47.0",
        "laravel/sanctum": "^3.3.3",
        "laravel/tinker": "^2.9"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.10.6",
        "fakerphp/faker": "^1.23.1",
        "laravel/pint": "^1.14.0",
        "laravel/sail": "^1.28.2",
        "mockery/mockery": "^1.6.7",
        "nunomaduro/collision": "^7.10",
        "pestphp/pest": "^2.34.1",
        "pestphp/pest-plugin-laravel": "^2.3",
        "pestphp/pest-plugin-livewire": "^2.1",
        "spatie/laravel-ignition": "^2.4.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models --nowrite"
        ],
        "post-install-cmd": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models --nowrite"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
